<?xml version="1.0" encoding="utf-8"?>
    <selector xmlns:android="http://schemas.android.com/apk/res/android">
        <!-- 無効状態 -->
        <item android:state_enabled="false" android:color="@color/colorSubtitleBar"/>
        <!-- 有効状態 -->
        <item android:state_enabled="true" android:color="@color/colorPrimary"/>
        <!-- チェック状態 -->
        <item android:color="@color/colorAccent" android:state_checked="true"/>
        <!-- タップしたとき -->
        <item android:color="@color/cardview_light_background" android:state_pressed="true"/>
        <!-- 通常 -->
        <item android:color="@color/colorBackOriginTerm"/>

</selector>
